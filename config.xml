<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns = "http://www.w3.org/ns/widgets" xmlns:gap = "http://phonegap.com/ns/1.0" id = "com.bigdogarcade" version = "1.0.0">
<name>Jumping Bird</name>
<description>This is my first phonegap build project using my Jumping Bird game. Built for my iPhone 4S</description>
<author href="http://www.bigdogarcade.com" email="rod@prowebmaker.com">Rod Snyder</author>
<feature name="http://api.phonegap.com/1.0/device" />
<preference name="phonegap-version" value="2.1.0" />
<preference name="orientation" value="portrait" />
<preference name="target-device" value="universal" />
<preference name="fullscreen" value="false" />
<preference name="webviewbounce" value="false" />
<preference name="prerendered-icon" value="true" />
<icon src="icon.png" />
<gap:splash src="assets/startscreen.png" gap:platform="ios" width="320" height="480" />
</widget>